{
    "version": 2,
    "builds": [
      {
        "src": "greenvolt_frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "./greenvolt_frontend/build" }
      },
      {
        "src": "SPA_consumo_energia/api_consumo.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/SPA_consumo_energia/api_consumo.py"
      },
      {
        "src": "/(.*)",
        "dest": "/greenvolt_frontend/build/$1"
      }
    ]
  }
  